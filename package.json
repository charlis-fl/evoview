{
  "name": "elizabeths-smile-react",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom --watch",
    "test:coverage": "react-scripts test --env=jsdom --watchAll --coverage --collectCoverageFrom='[\"src/**/*.{ts,tsx}\", \"!src/**/*.stories.tsx\"]'",
    "eject": "react-scripts eject",
    "lint": "npx eslint --fix --cache --ext .tsx --ext .ts . && tsc",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.10",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.5",
    "react-scripts": "^5.0.1",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.4",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-redux": "^7.1.24",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "chromatic": "^6.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "history": "^5.3.0",
    "msw": "^0.49.1",
    "webpack": "^5.75.0"
  },
  "overrides": {
    "@mdx-js/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-refresh": "0.11.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
